{
    "manifest_version": 2,
    "name": "ct-extension-v2",
    "version": "1.0",
    "description": "ct-extension",
    "content_scripts": [
        {
            "matches": ["<all_urls>", "https://www.youtube.com/*"],

            "js": ["content.js"]
        }
    ],

    "browser_action": {
        "default_icon": "1.png",
        "default_popup": "popup.html",
        "default_title": "A popup will come here."
    }
}
